<Page x:Class="ProductMonitorPt.View.SettingsPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProductMonitorPt.View"
        mc:Ignorable="d"
        Title="SettingsPage" Height="450" Width="800">
    <Grid>
        <UniformGrid  Rows="3"  Background="AliceBlue" HorizontalAlignment="Left"  VerticalAlignment="top" >
            <Label Content="设备清单"  x:Name="data1" VerticalContentAlignment="Center"/>
            <Label Content="环境监测"  x:Name="data2"  VerticalAlignment="Center"/>
            <Label Content="设备监测" x:Name="data3"   VerticalAlignment="Center"/>
            <Label Content="配置一"  x:Name="data4"  VerticalAlignment="Center"/>
            <Label Content="配置二"  x:Name="data5"  VerticalAlignment="Center"/>
        </UniformGrid>
    </Grid>
</Page>
